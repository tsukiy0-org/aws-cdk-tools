{
  "root": "packages/deployment",
  "sourceRoot": "packages/deployment/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/node:package",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "packages/deployment/dist",
        "tsConfig": "packages/deployment/tsconfig.lib.json",
        "packageJson": "packages/deployment/package.json",
        "main": "packages/deployment/src/index.ts",
        "assets": [
          "packages/deployment/*.md",
          {
            "input": "packages/deployment/static",
            "glob": "**/*",
            "output": "static"
          }
        ]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/deployment/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/packages/deployment"],
      "options": {
        "jestConfig": "packages/deployment/jest.config.js",
        "passWithNoTests": true
      }
    },
    "integration": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/packages/deployment"],
      "options": {
        "jestConfig": "packages/deployment/jest.config.integration.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
